iv className=\"container\">\n            <Breadcrumb className=\"dark\">\n                <Breadcrumb.Item href=\"/\">Home</Breadcrumb.Item>\n                <Breadcrumb.Item href=\"#/seguradoras\">\n                    Seguradoras\n                </Breadcrumb.Item>\n            </Breadcrumb>\n            <h2>Seguradoras</h2>\n        </div>\n    )\n}\n","/media/icc/Disco_main/Projetos javascript/rigonseguros/src/paraVoce/index.jsx",["93","94"],"import React from 'react'\nimport Breadcrumb from 'react-bootstrap/Breadcrumb'\nimport Col from 'react-bootstrap/Col'\nimport Container from 'react-bootstrap/Container'\nimport Row from 'react-bootstrap/Row'\nimport Jumbotron from 'react-bootstrap/Jumbotron'\nimport Accordion from 'react-bootstrap/Accordion'\nimport ListGroup from 'react-bootstrap/ListGroup'\nimport Card from 'react-bootstrap/Card'\nimport './index.css'\n\nimport Fipe from '../components/fipe/Fipe'\n\n\nexport default props => {\n\n\n    return (\n        <div className='container'>\n            <Breadcrumb className=\"dark\">\n                <Breadcrumb.Item href=\"/\">Home</Breadcrumb.Item>\n                <Breadcrumb.Item href=\"#/paravoce\">\n                    Para você\n        </Breadcrumb.Item>\n            </Breadcrumb>\n            <section id='seguro automovel'>\n                <Jumbotron id='auto'>\n                    <h1 className='text-white title'>Seguro Auto</h1>\n                </Jumbotron>\n                <Jumbotron>\n                    <h4 className=\"fs-1 text-center text-dark\">Confira as principais caracteristicas do seguro Automóvel</h4><br /><br />\n                    <Container>\n                        <Row>\n                            <Col>\n                                <Accordion >\n                                    <Card>\n                                        <Accordion.Toggle as={Card.Header} eventKey=\"0\">\n                                            <b>Serviços</b>\n                                        </Accordion.Toggle>\n                                        <Accordion.Collapse eventKey=\"0\">\n                                            <Card.Body>\n                                                <ListGroup>\n                                                    <ListGroup.Item>Guincho</ListGroup.Item>\n                                                    <ListGroup.Item>Mecânico para conserto provisório</ListGroup.Item>\n                                                    <ListGroup.Item>Carro reserva</ListGroup.Item>\n                                                    <ListGroup.Item>Troca de Pneus</ListGroup.Item>\n                                                    <ListGroup.Item>Chaveiro</ListGroup.Item>\n                                                </ListGroup>\n                                            </Card.Body>\n                                        </Accordion.Collapse>\n                                    </Card>\n                                    <Card>\n                                        <Accordion.Toggle as={Card.Header} eventKey=\"1\">\n                                            <b>Coberturas</b>\n                                        </Accordion.Toggle>\n                                        <Accordion.Collapse eventKey=\"1\">\n                                            <Card.Body>\n                                                <ListGroup>\n                                                    <ListGroup.Item><strong>Colisão, Incêndio e Roubo ou Furto</strong></ListGroup.Item>\n                                                    <ListGroup.Item><strong>Danos materiais e corporais</strong> causados a outras pessoas ou empresas</ListGroup.Item>\n                                                    <ListGroup.Item><strong>Garantia 0 km:</strong> garante que a indenização seja feita com base no valor de 0 km da tabela FIPE (índice utilizado para estabelecer o valor de mercado do veículo). Podendo ser contratado pelo período de 6 meses a partir do início do seguro.</ListGroup.Item>\n                                                    <ListGroup.Item><strong>Despesas extraordinárias:</strong> oferece um valor adicional de indenização, para cobrir despesas extras que possam surgir, no caso de indenização integral do veículo.</ListGroup.Item>\n                                                    <ListGroup.Item><strong>Despesas de Higienização do Veículo:</strong> garante a limpeza e a higienização do veículo em caso de alagamento por água de chuva ou inundações</ListGroup.Item>\n                                                    <ListGroup.Item><strong>Acidentes Pessoais de Passageiros (APP)</strong> do veículo segurado: garante cobertura para os passageiros do veículo segurado (incluindo o motorista) em caso de morte ou invalidez permanente, bem como para despesas médicas e hospitalares, que venham acontecer em um acidente coberto pelo seguro.</ListGroup.Item>\n                                                    <ListGroup.Item><strong>Acessórios e equipamentos:</strong> cobertura dos aparelhos de som/imagem, blindagem e/ou kit-gás</ListGroup.Item>\n                                                    <ListGroup.Item><strong>Danos morais:</strong> garante o reembolso de indenizações por danos morais causados a terceiros, em decorrência de um acidente.</ListGroup.Item>\n                                                </ListGroup>\n                                            </Card.Body>\n                                        </Accordion.Collapse>\n                                    </Card>\n                                    <Card>\n                                        <Accordion.Toggle as={Card.Header} eventKey=\"2\">\n                                            <b>Alta disponibilidade</b>\n                                        </Accordion.Toggle>\n                                        <Accordion.Collapse eventKey=\"2\">\n                                            <Card.Body ><p className=\"text-dark\" >Nosso segurado conta com atendimento especializado, disponível em  qualquer eventualidade. </p></Card.Body>\n                                        </Accordion.Collapse>\n                                    </Card>\n                                </Accordion>\n                            </Col>\n                            <Col>\n                                <p className='text-dark text-xl-start'>Além de todas as coberturas tradicionais de Colisão, Incêndio, Roubo e Danos à terceiros o seguro Auto Proporciona uma série de serviços que estão 24 horas a sua disposição para você Rodar Tranquilo</p>\n                                <a href=\"#/cotacao-auto\" className=\"btn btn-outline-primary\">Solicite cotação</a>\n                            </Col>\n                            <Col>\n                                <Fipe></Fipe>\n                            </Col>\n                        </Row>\n                    </Container>\n                </Jumbotron>\n            </section>\n            <section id='seguro residencia'>\n                <Jumbotron id='residencia'>\n                    <h1 className='text-white title'>Seguro residência</h1>\n                </Jumbotron>\n                <Jumbotron>\n                    <h4 className=\"fs-1 text-center text-dark\">Confira as principais caracteristicas do seguro residência</h4><br /><br />\n                    <Container>\n                        <Row>\n                            <Col>\n                                <Accordion >\n                                    <Card>\n                                        <Accordion.Toggle as={Card.Header} eventKey=\"0\">\n                                            <b>Serviços Básicos</b>\n                                        </Accordion.Toggle>\n                                        <Accordion.Collapse eventKey=\"0\">\n                                            <Card.Body>\n                                                <ListGroup>\n                                                    <ListGroup.Item>Vidraceiro </ListGroup.Item>\n                                                    <ListGroup.Item>Chaveiro</ListGroup.Item>\n                                                    <ListGroup.Item>Hidráulica</ListGroup.Item>\n                                                    <ListGroup.Item>Elétrica</ListGroup.Item>\n                                                    <ListGroup.Item>Limpeza da Residência</ListGroup.Item>\n                                                    <ListGroup.Item>Mudança de Móveis</ListGroup.Item>\n                                                    <ListGroup.Item>Guarda provisória de Móveis</ListGroup.Item>\n                                                    <ListGroup.Item>Cobertura Provisória de Telhados</ListGroup.Item>\n                                                    <ListGroup.Item>Hospedagem alternativa</ListGroup.Item>\n                                                    <ListGroup.Item>Regresso Antecipado</ListGroup.Item>\n                                                    <ListGroup.Item>Recuperação de Veículo</ListGroup.Item>\n                                                    <ListGroup.Item>Vigilância</ListGroup.Item>\n                                                </ListGroup>\n                                            </Card.Body>\n                                        </Accordion.Collapse>\n                                    </Card>\n                                    <Card>\n                                        <Accordion.Toggle as={Card.Header} eventKey=\"1\">\n                                            <b>Serviços Especiais</b>\n                                        </Accordion.Toggle>\n                                        <Accordion.Collapse eventKey=\"1\">\n                                            <Card.Body>\n                                                <ListGroup>\n                                                    <ListGroup.Item>Instalação de antenas VHF ou UHF (casa)</ListGroup.Item>\n                                                    <ListGroup.Item>Instalação de chuveiro elétrico, não blindado</ListGroup.Item>\n                                                    <ListGroup.Item>Instalação de olho mágico (porta de madeira)</ListGroup.Item>\n                                                    <ListGroup.Item>Instalação de torneira elétrica, não blindada</ListGroup.Item>\n                                                    <ListGroup.Item>Instalação de suporte para TV, até 70 polegadas</ListGroup.Item>\n                                                    <ListGroup.Item>Instalação de ventilador de teto (até 2 unidades)</ListGroup.Item>\n                                                    <ListGroup.Item>Troca de lâmpadas, tomadas e interruptores (até 10 unidades)</ListGroup.Item>\n                                                    <ListGroup.Item>Lubrificação de fechaduras e dobradiças</ListGroup.Item>\n                                                    <ListGroup.Item>Instalação de tela de segurança (apartamento)</ListGroup.Item>\n                                                </ListGroup>\n                                            </Card.Body>\n                                        </Accordion.Collapse>\n                                    </Card>\n                                    <Card>\n                                        <Accordion.Toggle as={Card.Header} eventKey=\"2\">\n                                            <b>Coberturas básicas</b>\n                                        </Accordion.Toggle>\n                                        <Accordion.Collapse eventKey=\"2\">\n                                            <Card.Body >\n                                                <ListGroup>\n                                                    <ListGroup.Item>Incêndio.</ListGroup.Item>\n                                                    <ListGroup.Item>Explosão.</ListGroup.Item>\n                                                    <ListGroup.Item>Fumaça.</ListGroup.Item>\n                                                    <ListGroup.Item>Queda de aeronaves (helicópteros, aviões, ou partes deles).</ListGroup.Item>\n                                                </ListGroup>\n                                            </Card.Body>\n                                        </Accordion.Collapse>\n                                    </Card>\n                                    <Card>\n                                        <Accordion.Toggle as={Card.Header} eventKey=\"3\">\n                                            <b>Coberturas Opcionais</b>\n                                        </Accordion.Toggle>\n                                        <Accordion.Collapse eventKey=\"3\">\n                                            <Card.Body >\n                                                <ListGroup>\n                                                    <ListGroup.Item>Responsabilidade civil familiar.</ListGroup.Item>\n                                                    <ListGroup.Item>Roubo com subtração de bens com vestígios</ListGroup.Item>\n                                                    <ListGroup.Item>Danos elétricos</ListGroup.Item>\n                                                    <ListGroup.Item>Pagamento ou Recebimento de aluguel no caso de sinistro​</ListGroup.Item>\n                                                    <ListGroup.Item>Equipamentos Eletrônicos</ListGroup.Item>\n                                                    <ListGroup.Item>Impacto de Veiculos</ListGroup.Item>\n                                                    <ListGroup.Item>Roubo de Bens no Depósito e/ou Bicicletário (Exclusivo para apartamento)</ListGroup.Item>\n                                                    <ListGroup.Item>Pagamento do condomínio em caso de perda de renda (Exclusivo para apartamento)</ListGroup.Item>\n                                                    <ListGroup.Item>Danos Causados aos Bens do Condomínio (Exclusivo para apartamento)</ListGroup.Item>\n                                                    <ListGroup.Item>Perda de Lucro e Despesas fixas devido incêndio por 3 meses (Exclusivo para Home Office)</ListGroup.Item>\n                                                </ListGroup>\n                                            </Card.Body>\n                                        </Accordion.Collapse>\n                                    </Card>\n                                </Accordion>\n                            </Col>\n                            <Col>\n                                <p className='text-dark text-xl-start'>O Seguro Residência protege sua casa ou apartamento, ou o que você está alugando, dos danos mais comuns que podem acontecer aos imóveis.</p>\n                                <a className=\"btn btn-outline-primary\">Solicite cotação</a>\n                            </Col>\n                        </Row>\n                    </Container>\n                </Jumbotron>\n            </section>\n            <section id='seguro vida'>\n                <Jumbotron id='vida'>\n                    <h1 className='text-white title'>seguro vida</h1>\n                </Jumbotron>\n                <Jumbotron>\n                    <h4 className=\"fs-1 text-center text-dark\">Confira as principais caracteristicas do seguro Vida Individual</h4><br /><br />\n                    <Container>\n                        <Row>\n                            <Col>\n                                <p className='text-dark text-xl-start'>O seguro Vida Individual é um seguro de vida completo, com amplas coberturas e vários serviços opcionais</p>\n                                <a href='/cotacao-auto' className=\"btn btn-outline-primary\">Solicite cotação</a>\n                            </Col>\n                            <Col>\n                                <Accordion >\n    